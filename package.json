    "@storybook/react": "^5.2.5",
    "@types/graphql": "^14.5.0",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router": "^5.1.2",
    "@types/styled-components": "^4.1.20",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "electron": "^7.0.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "fork-ts-checker-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "image-loader": "^0.0.1",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.15",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "apollo-server": "^2.9.7",
    "axios": "^0.19.0",
    "electron-is-dev": "^1.1.0",
    "final-form": "^4.18.5",
    "final-form-material-ui": "^0.3.0",
    "graphql": "^14.5.8",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-final-form": "^6.3.0",
    "react-router": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "styled-components": "^4.4.0",
    "type-graphql": "^0.17.5",
    "typedi": "^0.8.0",
    "typeface-roboto": "^0.0.75",
    "typeorm": "^0.2.20",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build": "npm run build-main && npm run build-renderer && npm run build-server",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-server-dev": "nodemon",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "build": {
    "productName": "invoice",
    "appId": "org.invoice.com",
    "mac": {
      "category": "public.app-category.business"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  }
}
