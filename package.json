  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build-server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build": "npm run build-main && npm run build-renderer && npm run build-server",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.dev.config.js --watch",
    "start-server-dev": "webpack --config webpack.server.dev.config.js --mode=development --watch",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder -p always",
    "postinstall": "electron-builder install-app-deps",
    "storybook": "cross-env STORYBOOK=true start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "schema-types": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "build": {
    "productName": "InvoiceR",
    "appId": "org.invoicer.com",
    "mac": {
      "category": "public.app-category.business"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  }
}
